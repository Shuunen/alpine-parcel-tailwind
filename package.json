{
  "author": "",
  "config": {
    "port": 8000
  },
  "description": "",
  "devDependencies": {
    "alpinejs": "^2.0.1",
    "cross-var": "1.1.0",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-html": "6.0.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "^1.12.4",
    "posthtml-include": "1.3.3",
    "rimraf": "3.0.2",
    "serve": "11.3.0",
    "tailwindcss": "^1.2.0"
  },
  "files": [
    "src"
  ],
  "keywords": [],
  "license": "ISC",
  "main": "index.js",
  "name": "alpine-parcel-tailwind",
  "scripts": {
    "build": "parcel build src/index.html --public-url . --no-source-maps",
    "check": "npx repo-check",
    "ci": "npm run test",
    "dev": "run-p watch ",
    "lint": "eslint --fix --ignore-path .gitignore --ext .js,.html .",
    "prebuild": "rimraf dist/*",
    "prestart": "npm run build",
    "serve": "cross-var serve dist --listen $npm_package_config_port --single",
    "start": "npm run serve",
    "test": "run-s check lint build",
    "update": "npx npm-check-updates -u",
    "watch": "cross-var parcel src/index.html --port $npm_package_config_port"
  },
  "version": "1.0.0"
}
